    "def Fibon(x,y,z):\n",
    "    i=0\n",
    "    L = [x,y]\n",
    "    while i < z-2:\n",
    "        L.append(L[i]+L[i+1])\n",
    "        i = i + 1\n",
    "    print(L)\n",
    "Fibon(1,4,10)"
